#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char *s;
int i,z=0,o=0,t=0,tt=0,f=0,fi=0,ss=0,se=0,e=0,n=0;
s=malloc(1024*sizeof(char));
scanf("%[^\n]",s);
s=realloc(s,strlen(s)+1);
int len=strlen(s);
    for(i=0;i<len;i++){
             if(s[i]=='0'){z++;}
        else if(s[i]=='1'){o++;}
        else if(s[i]=='2'){t++;}
        else if(s[i]=='3'){tt++;}
        else if(s[i]=='4'){f++;}
        else if(s[i]=='5'){fi++;}
        else if(s[i]=='6'){ss++;}
        else if(s[i]=='7'){se++;}
        else if(s[i]=='8'){e++;}
        else if(s[i]=='9'){n++;}
        
    }
    printf("%d %d %d %d %d %d %d %d %d %d",z,o,t,tt,f,fi,ss,se,e,n);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
